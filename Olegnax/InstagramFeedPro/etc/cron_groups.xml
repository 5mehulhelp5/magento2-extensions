<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/cron_groups.xsd">
    <group id="default_oxdaily">
        <schedule_generate_every>10</schedule_generate_every>
        <schedule_ahead_for>90000</schedule_ahead_for>
        <schedule_lifetime>90000</schedule_lifetime>
        <history_cleanup_every>10</history_cleanup_every>
        <history_success_lifetime>90000</history_success_lifetime>
        <history_failure_lifetime>90000</history_failure_lifetime>
        <use_separate_process>0</use_separate_process>
    </group>
</config>
